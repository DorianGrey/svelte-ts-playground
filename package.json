{
  "name": "svelte-ts-playground",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build --mode=production",
    "check": "svelte-check",
    "serve": "sirv dist -s -e",
    "test": "jest --watch",
    "test:ci": "jest --no-cache --coverage",
    "e2e": "jest -c jest.e2e.config.js --runInBand",
    "lint": "eslint \"{src,e2e}/**/*.ts\"",
    "format": "prettier --write --plugin-search-dir=. \"{src,e2e}/**/*.{ts,svelte}\""
  },
  "dependencies": {
    "page": "^1.11.6",
    "svelte": "^3.24.0",
    "svelte-i18n": "^3.0.4",
    "tailwindcss": "^1.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "expect-playwright": "^0.2.5",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "jest-playwright-preset": "^1.3.1",
    "lint-staged": "^10.3.0",
    "playwright-chromium": "^1.4.0",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "sirv-cli": "^1.0.6",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.2.1",
    "typescript": "^3.9.3",
    "vite": "^1.0.0-rc.1",
    "vite-plugin-svelte": "^2.4.1"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=5.10",
    "pnpm": ">=5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm check && lint-staged",
      "pre-push": "pnpm test:ci"
    }
  },
  "jest-junit": {
    "outputDirectory": "./test-results/junit/"
  },
  "browserslist": ">0.25%, not ie 11, not op_mini all"
}
